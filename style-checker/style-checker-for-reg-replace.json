"regreplace.commands": [
  {"name": "2", "regexp": "\\t(?!<%= vars)", "replace": "  "},
  {"name": "3",  "regexp": "(-SERVICE-INSTANCE-NAME\\b[^`]*(```\\s|```$))", "replace": "$1<!--฿ Just |SERVICE-INSTANCE-NAME| is preferred. ฿-->"},
  {"name": "4",  "regexp": "(-v\\s+\\d\\.\\d\\.\\d[\\n\\s.][^`]*(```\\s|```$))", "replace": "$1<!--฿ If this is a command to run and it has a version number, consider replacing the version number with an HBS variable, such as {{ vars.tap_version }}. ฿-->"},
  {"name": "5",  "regexp": "(:(?=\\n\\n\\|\\s*Option))", "replace": "$1<!--฿ A sentence that introduces a table of options ends in a period, not a colon. ฿-->"},
  {"name": "6",  "regexp": "(!\\[.*[^A-Za-z0-9\\s\\.,-:].*\\]\\(.*\\.png\\))", "replace": "$1<!--฿ If you must use a special character in alt text, spell it out. For example, write |hashtag| instead of |#|. ฿-->"},
  {"name": "7",  "regexp": "(!\\[.*graphic.*\\]\\(.*\\.png\\))", "replace": "$1<!--฿ Do not use phrases such as |graphic of| or |image of| in alt text. ฿-->"},
  {"name": "8",  "regexp": "(!\\[.*image.*\\]\\(.*\\.png\\))", "replace": "$1<!--฿ Do not use phrases such as |graphic of| or |image of| in alt text. ฿-->"},
  {"name": "9",  "regexp": "(!\\[.*Screen[\\s\\n]+Shot.*\\]\\(.*\\.png\\))", "replace": "$1<!--฿ |Screenshot| is one word. Consider whether the word is really needed in the alt text. ฿-->"},
  {"name": "10",  "regexp": "(!\\[.*Screen[\\s\\n]+shot.*\\]\\(.*\\.png\\))", "replace": "$1<!--฿ |Screenshot| is one word. Consider whether the word is really needed in the alt text. ฿-->"},
  {"name": "11",  "regexp": "(!\\[.*screen[\\s\\n]+shot.*\\]\\(.*\\.png\\))", "replace": "$1<!--฿ |screenshot| is one word. Consider whether the word is really needed in the alt text. ฿-->"},
  {"name": "12",  "regexp": "(!\\[[^\\]]{0,44}\\]\\(.*\\.png\\))", "replace": "$1<!--฿ Alt text must describe the image in detail. ฿-->"},
  {"name": "13",  "regexp": "(!\\[[^\\]]{151,}\\]\\(.*\\.png\\))", "replace": "$1<!--฿ Reduce alt text to 150 characters or fewer. ฿-->"},
  {"name": "14",  "regexp": "(!\\[[^\\]]*[^\\.]]\\(.*\\.png\\))", "replace": "$1<!--฿ In alt text, write a coherent sentence that ends with a period. ฿-->"},
  {"name": "15",  "regexp": "([\\*\\+\\-]\\s`[A-Za-z-]*_.*(`\\s|`$))", "replace": "$1<!--฿ If this is a placeholder, use dashes for spaces instead of underscores. ฿-->"},
  {"name": "16",  "regexp": "([\\s\\n]+\\*\\*[^*]{96,})", "replace": "$1<!--฿ Consider reducing this run-in heading to fewer than 100 characters. ฿-->"},
  {"name": "17",  "regexp": "([^\\s\\n]<\\/em>(?![\\s\\n<\\.:,-]))", "replace": "$1<!--฿ Needed whitespace is likely missing after the closing emphasis tag. ฿-->"},
  {"name": "18",  "regexp": "([^\\s\\n]<\\/strong>(?![\\s\\n<\\.:,-]))", "replace": "$1<!--฿ Needed whitespace is likely missing after the closing strong tag. ฿-->"},
  {"name": "19",  "regexp": "([^\\s\\n>](?=<em>))", "replace": "$1<!--฿ Needed whitespace is likely missing before the opening emphasis tag. ฿-->"},
  {"name": "20",  "regexp": "([^\\s\\n>](?=<strong>))", "replace": "$1<!--฿ Needed whitespace is likely missing before the opening strong tag. ฿-->"},
  {"name": "21",  "regexp": "(\\.[a-z]+#[a-z-]+-\\d+\\))", "replace": "$1<!--฿ Find the anchor this link points to and give it a short HTML anchor ID, such as |anchor-id| -- keep it under 25 chars and use dashes instead of spaces -- then change this link to point to that HTML anchor ID. ฿-->"},
  {"name": "22",  "regexp": "(\\.\\/[a-z-]*#[a-z-]*\\.html)", "replace": "$1<!--฿ Broken link -- the correct syntax is |page-name.html#anchor-id|. ฿-->"},
  {"name": "23",  "regexp": "(\\.yml)", "replace": "$1<!--฿ Per YAML.org, |.yaml| is the preferred extension. If the reader creates this YAML file, fix every instance of the filename in every topic as part of your commit. Files with the |.yml| extension downloaded from elsewhere are outside our control. ฿-->"},
  {"name": "24",  "regexp": "(\\[(?!.*\\n?.*AWS documentation).*\\]\\(https:..docs.aws.amazon.com.*\\))", "replace": "$1<!--฿ Change the link name to |AWS documentation|. ฿-->"},
  {"name": "25",  "regexp": "(\\[(?!.*\\n?.*BOSH documentation).*\\]\\(https:..bosh.io.*\\))", "replace": "$1<!--฿ Change the link name to |BOSH documentation|. ฿-->"},
  {"name": "26",  "regexp": "(\\[(?!.*\\n?.*cert-manager documentation).*\\]\\(https:..cert-manager.io.docs.*\\))", "replace": "$1<!--฿ Change the link name to |cert-manager documentation|. ฿-->"},
  {"name": "27",  "regexp": "(\\[(?!.*\\n?.*Cloud Foundry documentation).*\\]\\(https:..docs.cloudfoundry.org.*\\))", "replace": "$1<!--฿ Change the link name to |Cloud Foundry documentation|. ฿-->"},
  {"name": "28",  "regexp": "(\\[(?!.*\\n?.*Concourse documentation).*\\]\\(https:..concourse-ci.org.*\\))", "replace": "$1<!--฿ Change the link name to |Concourse documentation|. ฿-->"},
  {"name": "29",  "regexp": "(\\[(?!.*\\n?.*Docker documentation).*\\]\\(https:..docs.docker.com.*\\))", "replace": "$1<!--฿ Change the link name to |Docker documentation|. ฿-->"},
  {"name": "30",  "regexp": "(\\[(?!.*\\n?.*documentation).*\\]\\(https:..cloud.google.com.*\\))", "replace": "$1<!--฿ Change the link name to |GCP documentation| or |Google Cloud documentation|, whichever applies. ฿-->"},
  {"name": "31",  "regexp": "(\\[(?!.*\\n?.*Istio documentation).*\\]\\(https:..istio.io.latest.docs.*\\))", "replace": "$1<!--฿ Change the link name to |Istio documentation|. ฿-->"},
  {"name": "32",  "regexp": "(\\[(?!.*\\n?.*Kubernetes documentation).*\\]\\(https:..kubernetes.io.docs.*\\))", "replace": "$1<!--฿ Change the link name to |Kubernetes documentation|. ฿-->"},
  {"name": "33",  "regexp": "(\\[(?!.*\\n?.*Microsoft documentation).*\\]\\(https:..docs.microsoft.com.*\\))", "replace": "$1<!--฿ Change the link name to |Microsoft documentation|. ฿-->"},
  {"name": "34",  "regexp": "(\\[(?!.*\\n?.*Percona documentation).*\\]\\(https:..www.percona.com.doc.*\\))", "replace": "$1<!--฿ Change the link name to |Percona documentation|. ฿-->"},
  {"name": "35",  "regexp": "(\\[(?!.*\\n?.*RabbitMQ documentation).*\\]\\(https:..www.rabbitmq.com.*\\))", "replace": "$1<!--฿ Change the link name to |RabbitMQ documentation|. ฿-->"},
  {"name": "36",  "regexp": "(\\[(?!.*\\n?.*Terraform documentation).*\\]\\(https:..www.terraform.io.docs.*\\))", "replace": "$1<!--฿ Change the link name to |Terraform documentation|. ฿-->"},
  {"name": "37",  "regexp": "(\\[(?!.*\\n?.*VMware Tanzu Support knowledge base).*\\]\\(https:..community.pivotal.io.*\\))", "replace": "$1<!--฿ Change the link name to |VMware Tanzu Support documentation|. ฿-->"},
  {"name": "38",  "regexp": "(\\[[^\\]]*`[^\\]]*]\\([^)]*\\))", "replace": "$1<!--฿ Do not use backticks in link names. ฿-->"},
  {"name": "39",  "regexp": "(\\[[^\\]]+\\]\\(\\/(?!root).*\\))", "replace": "$1<!--฿ Do not start a relative link with a forward slash. ฿-->"},
  {"name": "40",  "regexp": "(\\].(?=\\((http|#)))", "replace": "$1<!--฿ Broken link ฿-->"},
  {"name": "41",  "regexp": "(\\]\\([^\\(:]*\\.[^\\/\\(]*\\.hbs\\.md[#a-z-]*\\))", "replace": "$1<!--฿ Do not use periods in filenames. DocWorks reports them as errors. ฿-->"},
  {"name": "42",  "regexp": "(\\]\\([^\\(:]*\\.[^\\/\\(]*\\.html[#a-z-]*\\))", "replace": "$1<!--฿ Do not use periods in filenames. DocWorks reports them as errors. ฿-->"},
  {"name": "43",  "regexp": "(\\]\\([^\\(:]*\\.[^\\/\\(]*\\.png\\))", "replace": "$1<!--฿ Do not use periods in filenames. DocWorks reports them as errors. ฿-->"},
  {"name": "44",  "regexp": "(\\*\\*[\\s\\n]+is[\\s\\n]+inactive\\b)", "replace": "$1<!--฿ |dimmed| or |unavailable| is the preferred term for a greyed-out UI object. ฿-->"},
  {"name": "45",  "regexp": "(\\*\\*Warning)", "replace": "$1<!--฿ |Caution| is preferred over |Warning|. ฿-->"},
  {"name": "46",  "regexp": "(\\|(?!(\\n))\\s*\\|)", "replace": "$1<!--฿ Insert |_n/a_| within purposefully empty cells. ฿-->"},
  {"name": "47",  "regexp": "(\\|(?!(\\n))\\s*n\\/a)", "replace": "$1<!--฿ Insert |_n/a_| within purposefully empty cells. ฿-->"},
  {"name": "48",  "regexp": "(\\|(?=\\n\\n+\\|[^\\|]*\\|))", "replace": "$1<!--฿ Do not stack tables. ฿-->"},
  {"name": "49",  "regexp": "(\\|\\s*.*\\.\\.\\..*\\|[\\s\\n]+\\|-+\\|)", "replace": "$1<!--฿ Do not use ellipsis in a column header. ฿-->"},
  {"name": "50",  "regexp": "(\\bAPP(?!-NAME)[^`]*(```\\s|```$))", "replace": "$1<!--฿ |APP-NAME| is preferred. ฿-->"},
  {"name": "51",  "regexp": "(\\bChoose[\\s\\n]+\\*\\*)", "replace": "$1<!--฿ |Select| or |Click| is preferred for interacting with UI elements. ฿-->"},
  {"name": "52",  "regexp": "(\\bchoose[\\s\\n]+\\*\\*)", "replace": "$1<!--฿ |select| or |click| is preferred for interacting with UI elements. ฿-->"},
  {"name": "53",  "regexp": "(\\bNavigate[\\s\\n]+to(?=[\\s\\n]+\\[[^\\]]+\\]\\())", "replace": "$1<!--฿ |Go to| is preferred for URLs. ฿-->"},
  {"name": "54",  "regexp": "(\\bnavigate[\\s\\n]+to(?=[\\s\\n]+\\[[^\\]]+\\]\\())", "replace": "$1<!--฿ |go to| is preferred for URLs. ฿-->"},
  {"name": "55",  "regexp": "(\\bPRIVATE-KEY-FILE\\b[^`]*(```\\s|```$))", "replace": "$1<!--฿ |PATH-TO-PRIVATE-KEY| is preferred. ฿-->"},
  {"name": "56",  "regexp": "(\\brun:[\\s\\n]*<code>[\\s\\n\\~]*\\$)", "replace": "$1<!--฿ We only include dollar signs in example terminal code, not in commands the reader might copy and paste. ฿-->"},
  {"name": "57",  "regexp": "(\\brun[^:]*:[\\s\\n]*<pre class)", "replace": "$1<!--฿ Commands are formatted in triple backtick blocks, not terminal snippets. ฿-->"},
  {"name": "58",  "regexp": "(\\brunning[^:]*:[\\s\\n]*<code>[\\s\\n\\~]*\\$)", "replace": "$1<!--฿ We only include dollar signs in example terminal code, not in commands the reader might copy and paste. ฿-->"},
  {"name": "59",  "regexp": "(\\bSERVICE-INSTANCE[\\s\\n]+[^`]*(```\\s|```$))", "replace": "$1<!--฿ |SERVICE-INSTANCE-NAME| is preferred. ฿-->"},
  {"name": "60",  "regexp": "(\\bType(?=[\\s\\n]+`))", "replace": "$1<!--฿ Use |enter| when the user input appears on the screen. Use |run| when you want users to run commands. ฿-->"},
  {"name": "61",  "regexp": "(\\btype(?=[\\s\\n]+`))", "replace": "$1<!--฿ Use |enter| when the user input appears on the screen. Use |run| when you want users to run commands. ฿-->"},
  {"name": "62",  "regexp": "(\\bYOUR-PRIVATE-KEY\\b[^`]*(```\\s|```$))", "replace": "$1<!--฿ |PATH-TO-PRIVATE-KEY| is preferred. ฿-->"},
  {"name": "63",  "regexp": "(\\bYOUR-VM\\b[^`]*(```\\s|```$))", "replace": "$1<!--฿ Specify the VM, as in |OPS-MANAGER-VM| or |STEMCELL-BASE-VM|. ฿-->"},
  {"name": "64",  "regexp": "(\\n```[^`]*[A-Z][A-Z]+[\\-\\_][^`]*(```\\s|```$)(?![\\s\\n]*Where))", "replace": "$1<!--฿ Verify that no placeholders above require explanation in the style of |Where PLACEHOLDER is...| ฿-->"},
  {"name": "65",  "regexp": "(\\sCVE-\\d{4}(?!.*\\n?([^\\[]*\\]|[^>]*<\\/a>)))", "replace": "$1<!--฿ Link to the CVE page at https://nvd.nist.gov/ or, if unavailable, cve.org ฿-->"},
  {"name": "66",  "regexp": "(##+[^\\n]*>\\s*(?=[A-Z][a-z-]+ing\\b.*\\n\\n(.+\\n){1,}\\n+\\d\\.))", "replace": "$1<!--฿ Procedure headers use imperatives, not gerunds. ฿-->"},
  {"name": "67",  "regexp": "(##+\\s*<a\\sid=[^>]+>\\s*<\\/a>\\s*\\d\\.)", "replace": "$1<!--฿ Do not format a numbered step as a header. ฿-->"},
  {"name": "68",  "regexp": "(`.*[_\\A-Z].*`[\\s\\n]+errand\\b)", "replace": "$1<!--฿ Errands tend to be lowercase and they use dashes instead of underscores or spaces. ฿-->"},
  {"name": "69",  "regexp": "(`.*[_\\A-Z].*`[\\s\\n]+errands\\b)", "replace": "$1<!--฿ Errands tend to be lowercase and they use dashes instead of underscores or spaces. ฿-->"},
  {"name": "70",  "regexp": "```(\\n|java|javascript|js|handlebars|hbs)[^`]*[^\\\\]\\{\\{[^`]*(```\\s|```$)", "replace": "$1<!--฿ If the double curly braces are not HBS and within a coding language that uses curly braces, and this file has the .hbs extension, add a backslash before the opening double curly braces to escape them. ฿-->"},
  {"name": "71",  "regexp": "(```[^<]*(<strong>|<em>|<h\\d>|<html>|<body>)(?![^<]*<\\/pre>)[^`]*(```\\s|```$))", "replace": "$1<!--฿ Use unicode for angle brackets to ensure they show up in the snippet. |&lt;| for a left angle bracket and |&gt;| for a right angle bracket. ฿-->"},
  {"name": "72",  "regexp": "(```bash\\b[^`]*(```\\s|```$))", "replace": "$1<!--฿ |console| is usually the better fence info string to use because the reader typically has a choice of CLI. ฿-->"},
  {"name": "73",  "regexp": "(```console\n.{105,}[^`]*(```\\s|```$))", "replace": "$1<!--฿ If this is just console output, such as an error message, break up the lines at sensible points with backslashes to make reading it easier. ฿-->"},
  {"name": "74",  "regexp": "(```kubectl\\b[^`]*(```\\s|```$))", "replace": "$1<!--฿ |kubectl| is not in the Linguist library. Use |console| instead. ฿-->"},
  {"name": "75",  "regexp": "(```sh\\b[^`]*(```\\s|```$))", "replace": "$1<!--฿ |console| is usually the better fence info string to use because the reader typically has a choice of CLI. ฿-->"},
  {"name": "76",  "regexp": "(```shell\\b[^`]*(```\\s|```$))", "replace": "$1<!--฿ |console| is usually the better fence info string to use because the reader typically has a choice of CLI. ฿-->"},
  {"name": "77",  "regexp": "(`kubeconfig`)", "replace": "$1<!--฿ Delete the code tags because kubeconfig is not a filename, although it does look like one. ฿-->"},
  {"name": "78",  "regexp": "(`kubectl(`\\s|`\\.|`$))", "replace": "$1<!--฿ Do not format |kubectl| as code. ฿-->"},
  {"name": "79",  "regexp": "(`tanzu`[\\s\\n]+CLI\\b)", "replace": "$1<!--฿ |the Tanzu CLI| is preferred. No code tags. ฿-->"},
  {"name": "80",  "regexp": "(^.*`.*(?=\\n:\\s[A-Za-z]+))", "replace": "$1<!--฿ Do not use backticks in tab titles. ฿-->"},
  {"name": "81",  "regexp": "(^[\\s]*(?!<li>)[^<]*<a href=.#[^>]*.>(?![^\\.]*\\n?[^\\.]*(earlier|previously|before|later)))", "replace": "$1<!--฿ After giving the anchor, state whether the target is |earlier| or |later| in the topic. ฿-->"},
  {"name": "82",  "regexp": "(^\\[[^\\]]*\\]\\(#[^\\)]*\\)(?![^\\.]*\\n?[^\\.]*(earlier|previously|before|later)))", "replace": "$1<!--฿ After giving the anchor, state whether the target is |earlier| or |later| in the topic. ฿-->"},
  {"name": "83",  "regexp": "(^\\d\\.\\s+.*example.*\\.[\\s\\n]*(?=```))", "replace": "$1<!--฿ When introducing an example in a step, end with a colon. ฿-->"},
  {"name": "84",  "regexp": "(^\\d\\.\\s+[^:]*:\\s*\\n\\s+\\*)", "replace": "$1<!--฿ A step that has sub-steps ends in a period, not a colon. ฿-->"},
  {"name": "85",  "regexp": "(^\\n+#\\s*(?=<a\\s+id))", "replace": "$1<!--฿ Do not use anchor IDs for level 1 headers in DocsWorks content. ฿-->"},
  {"name": "86",  "regexp": "(^#\\s*<a\\s+id=[^>]*><\\/a>)", "replace": "$1<!--฿ Do not use anchor IDs for level 1 headers in DocsWorks content. ฿-->"},
  {"name": "87",  "regexp": "(^#\\s+[^#]*\\n(\\n[\\*\\+-]\\s+\\[[^\\]]+\\]\\([^\\)]+\\)){2,})", "replace": "$1<!--฿ If this is a topic ToC, maybe delete it. It is work to maintain it, and the jumplist in the subnav is obvious. ฿-->"},
  {"name": "88",  "regexp": "(^#\\s+[A-Z][a-z-]+ing\\b(?=[^₣]*\\n\\s*1\\.))", "replace": "$1<!--฿ If a procedure topic, consider using an imperative instead of a gerund. ฿-->"},
  {"name": "89",  "regexp": "(^##+\\s*.*\\n+\\|[^\\|]*\\|)", "replace": "$1<!--฿ Do not place a table immediately beneath a header. ฿-->"},
  {"name": "90",  "regexp": "(^##+\\s*.*\\n+>\\s*\\*\\*)", "replace": "$1<!--฿ Do not place a note immediately beneath a header. ฿-->"},
  {"name": "91",  "regexp": "(^##+\\s*<a\\s+id=[^>]*><\\/a>\\s*[a-z][a-z]+)", "replace": "$1<!--฿ Headers start with an uppercase letter. ฿-->"},
  {"name": "92",  "regexp": "(^##+\\s*<a\\s+id=[^>]*><\\/a>\\s*\\d\\.\\s*[a-z])", "replace": "$1<!--฿ Headers start with an uppercase letter. ฿-->"},
  {"name": "93",  "regexp": "(^##+\\s+(?!<a\\s+id))", "replace": "$1<!--฿ Add an HTML anchor ID unless you have reason not to. Keep it under 26 chars. Use dashes instead of spaces and periods. ฿-->"},
  {"name": "94",  "regexp": "(^##+\\s+<a id=[^>]+>(?!<\\/a>))", "replace": "$1<!--฿ Add the closing |</a>| immediately after the anchor ID for headers. ฿-->"},
  {"name": "95",  "regexp": "(<[A-Z]+_[A-Z_]+>[^`]*(```\\s|```$))", "replace": "$1<!--฿ If a placeholder, delete the angle brackets and use dashes instead of underscores. ฿-->"},
  {"name": "96",  "regexp": "(<[A-Z]+-[A-Z_]+>[^`]*(```\\s|```$))", "replace": "$1<!--฿ If a placeholder, delete the angle brackets. ฿-->"},
  {"name": "97",  "regexp": "(<[A-Z]+\\\\\\s[A-Z_]+>[^`]*(```\\s|```$))", "replace": "$1<!--฿ If a placeholder, delete the angle brackets and use dashes instead of escaped spaces. ฿-->"},
  {"name": "98",  "regexp": "(<[A-Z]+\\s[A-Z_]+>[^`]*(```\\s|```$))", "replace": "$1<!--฿ If a placeholder, delete the angle brackets and use dashes instead of spaces. ฿-->"},
  {"name": "99",  "regexp": "(<a\\s+id..[\\w-]{26,}.*)", "replace": "$1<!--฿ Shorten the anchor ID to 25 characters or fewer. Use dashes instead of spaces or periods. ฿-->"},
  {"name": "100",  "regexp": "(<a\\s+id..[\\w-]+\\..*)", "replace": "$1<!--฿ Use dashes instead of spaces or periods in anchor IDs. ฿-->"},
  {"name": "101",  "regexp": "(<a\\s+id..[\\w-]+\\s.*)", "replace": "$1<!--฿ Use dashes instead of spaces or periods in anchor IDs. ฿-->"},
  {"name": "102",  "regexp": "(<code>kubeconfig<\\/code>)", "replace": "$1<!--฿ Delete the code tags because kubeconfig is not a filename, although it does look like one. ฿-->"},
  {"name": "103",  "regexp": "(<code>kubectl<\\/code>)", "replace": "$1<!--฿ Do not format |kubectl| as code. ฿-->"},
  {"name": "104",  "regexp": "(<h1>)", "replace": "$1<!--฿ HTML H1 headers are not allowed within Bookbinder-built topics. ฿-->"},
  {"name": "105",  "regexp": "(<td>\\s*n\\/a)", "replace": "$1<!--฿ Insert |<em>n/a</em>| within purposefully empty cells. ฿-->"},
  {"name": "106",  "regexp": "(<td><\\/td>)", "replace": "$1<!--฿ Insert |<em>n/a</em>| within purposefully empty cells. ฿-->"},
  {"name": "107",  "regexp": "(docs-pcf-staging.cfapps.io[^\\s]*\\))", "replace": "$1<!--฿ Do not link to the staging version unless A. this is the main branch and B. there is no prod link for this page right now. ฿-->"},
  {"name": "108",  "regexp": "(html#[a-z-]*\\)[\\s\\n]+earlier\\b)", "replace": "$1<!--฿ Delete |earlier|: the reference is in another topic. ฿-->"},
  {"name": "109",  "regexp": "(html#[a-z-]*\\)[\\s\\n]+later\\b)", "replace": "$1<!--฿ Delete |later|: the reference is in another topic. ฿-->"},
  {"name": "110",  "regexp": "(https:..docs-staging[^\\s]*\\))", "replace": "$1<!--฿ Do not link to the staging version unless A. this is the main branch and B. there is no prod link for this page right now. ฿-->"},
  {"name": "111",  "regexp": "(PATH-TO-MANIFEST[^\\.][^`]*(```\\s|```$))", "replace": "$1<!--฿ Include the file extension, as in |PATH-TO-MANIFEST.yaml|. ฿-->"},
  {"name": "112",  "regexp": "(PATH-TO-VARIABLES[^\\.][^`]*(```\\s|```$))", "replace": "$1<!--฿ Include the file extension, as in |PATH-TO-VARIABLES.yaml|. ฿-->"},
  {"name": "113",  "regexp": "(run:[\\n\\s]+```[^`]+\\n\\s*\\$[^`]*(```\\s|```$))", "replace": "$1<!--฿ Use the dollar sign only inside example terminal code, not in commands to run. ฿-->"},
  {"name": "114",  "regexp": "(running:[\\n\\s]+```[^`]+\\n\\s*\\$[^`]*(```\\s|```$))", "replace": "$1<!--฿ Use the dollar sign only inside example terminal code, not in commands to run. ฿-->"},
  {"name": "115",  "regexp": "(Where `[A-Za-z]*_[^`]*(`\\s|`$))", "replace": "$1<!--฿ If this is a placeholder, use dashes for spaces instead of underscores. ฿-->"},
  {"name": "116",  "regexp": "(---\\n\\n[^#\\n]*(?=(#[^#]|###))(?!`[\\s\\n\\.:,]|\\*\\*[\\s\\n\\.:,]|[^`]*[^\\s\\n]`[\\s\\n\\.:,]|[^`]*`$|[^`]*```[\\s\\n]|[^`]*```$|[^<]*<\\/code>|[^\\*:]*\\*\\*[\\s\\n\\.:,]|[^฿]*฿-->|[^\\[]*\\][^`]|[^\\(]*\\)[^`]|[^\\.\\s]*\\.png|[^\\s]*>|[^{]*}}))", "replace": "$1<!--฿ The first header must be ## -- no smaller, no larger. ฿-->"},
